void setup(){
  size(500, 500);
  background(0);
    
}

void draw(){
  stroke(255, 255, 255);
  line(200, 200, 300, 200);
  line(200, 300, 300, 300);
  line(200, 200, 200, 300);
  line(300, 300, 300, 200);
  
  line(150, 150, 250, 150);
  line(150, 250, 250, 250);
  line(150, 150, 150, 250);
  line(250, 150, 250, 250);
  
  line(150, 150, 200, 200);
  line(150, 250, 200, 300);
  line(250, 250, 300, 300);
  line(250, 150, 300, 200);
  
  if(!mousePressed){    
    fill(0, 0, 255);
    square(200, 200, 100);    
  }else{
    fill(0, 0, 255);
    square(150, 150, 100);
  }
}
